{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "address": {
      "type": "text"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "pincode": {
      "type": "integer"
    },
    "amount": {
      "type": "biginteger"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "user_order"
    },
    "product": {
      "model": "product",
      "via": "orders"
    }
  }
}
